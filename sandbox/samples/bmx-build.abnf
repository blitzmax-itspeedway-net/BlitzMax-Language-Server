program = <c-eol> <programbody>

testprogram = <strictmode> <framework> | <module> <programbody>

c-eol = comment | EOL

strictmode = "strict" | "superstrict" comment EOL

framework = "framework" ALPHA "." ALPHA

programbody = <print> ; Will also have functiondef, typedef etc!

print = "print" QSTRING EOL


